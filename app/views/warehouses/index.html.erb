<p id="notice"><%= notice %></p>

<h1>Warehouses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Owner name</th>
      <th>State</th>
      <th>District</th>
      <th>Post office</th>
      <th>Police station</th>
      <th>Pincode</th>
      <th>Postal address</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @warehouses.each do |warehouse| %>
      <tr>
        <td><%= warehouse.name %></td>
        <td><%= warehouse.owner_name %></td>
        <td><%= warehouse.state %></td>
        <td><%= warehouse.district %></td>
        <td><%= warehouse.post_office %></td>
        <td><%= warehouse.police_station %></td>
        <td><%= warehouse.pincode %></td>
        <td><%= warehouse.postal_address %></td>
        <td><%= warehouse.status %></td>
        <td><%= link_to 'Show', warehouse %></td>
        <td><%= link_to 'Edit', edit_warehouse_path(warehouse) %></td>
        <td><%= link_to 'Destroy', warehouse, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Warehouse', new_warehouse_path %>
